        Color="#333337" />

    <LinearGradientBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:ResourceKeys}, ResourceId=MenuBackgroundKey}" o:Freeze="True" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="1" Color="#1B1B1C" />
    </LinearGradientBrush>

    <!--  Styles the background of the top level item in a menu (Files, Edit, ...)  -->
    <SolidColorBrush
        x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:ResourceKeys},
                                     ResourceId=TopLevelHeaderMenuBackgroundKey}"
        o:Freeze="True"
        Color="#333337" />
    <!--  Control Keys XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX  -->

    <SolidColorBrush
        x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:ResourceKeys},
                                     ResourceId=TextBrushKey}"
        o:Freeze="True"
        Color="White" />

    <SolidColorBrush
        x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:ResourceKeys},
                                     ResourceId=ItemBackgroundSelectedKey}"
        o:Freeze="True"
        Color="{DynamicResource {x:Static r:ResourceKeys.ControlAccentColorKey}}" />

    <SolidColorBrush
        x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:ResourceKeys},
                                     ResourceId=ItemTextDisabledKey}"
        o:Freeze="True"
        Color="#717171" />

    <SolidColorBrush
        x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:ResourceKeys},
                                     ResourceId=NormalBackgroundBrushKey}"
        o:Freeze="True"
        Color="#FF2D2D30" />

    <SolidColorBrush
        x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:ResourceKeys},
                                     ResourceId=ItemBackgroundHoverKey}"
        o:Freeze="True"
        Color="#3e3e42" />

    <!--  Define Shadow Effect for each menu item  -->
    <DropShadowEffect
        x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:ResourceKeys},
                                     ResourceId=DropShadowEffectKey}"
        ShadowDepth="2"
        Color="Black" />

    <!--#endregion-->

    <!--#region UserDialogWindow style brushes-->

    <SolidColorBrush
        x:Key="{x:Static r:ResourceKeys.UserDialogWindowBackground}"
        o:Freeze="True"
        Color="#404040" />

    <!--#endregion-->

</ResourceDictionary>