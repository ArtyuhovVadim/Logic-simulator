    <!--#region AvaloneDockMenu-->

    <!--  Menu  -->
    <SolidColorBrush
        x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:AvalonDockResourceKeys},
                                     ResourceId=MenuSeparatorBorderBrushKey}"
        o:Freeze="True"
        Color="#333337" />

    <SolidColorBrush
        x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:AvalonDockResourceKeys},
                                     ResourceId=SubmenuItemBackgroundKey}"
        o:Freeze="True"
        Color="#1B1B1C" />

    <!--  Highlighing Top Menu Entry on Mouse over  -->
    <SolidColorBrush
        x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:AvalonDockResourceKeys},
                                     ResourceId=MenuItemHighlightedBackgroundKey}"
        o:Freeze="True"
        Color="#3E3E40" />

    <!--  Highlighting all other than Top Menu entry on mouse over  -->
    <SolidColorBrush
        x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:AvalonDockResourceKeys},
                                     ResourceId=SubmenuItemBackgroundHighlightedKey}"
        o:Freeze="True"
        Color="#333334" />

    <!--  XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX  -->
    <SolidColorBrush
        x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:AvalonDockResourceKeys},
