        Color="{DynamicResource {x:Static r:AvalonDockResourceKeys.ControlAccentColorKey}}" />
    <SolidColorBrush
        x:Key="{x:Static r:AvalonDockResourceKeys.PreviewBoxBackgroundBrushKey}"
        o:Freeze="true"
